<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FireApp.AppFlyout"
             xmlns:views="clr-namespace:FireApp.Views"
            Shell.FlyoutWidth="200"
            FlyoutLayoutBehavior="Popover"
            >
    <FlyoutPage.Flyout>
        <views:FlyoutMenu x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <views:LoginPage />
                <!--<views:Inicial />-->
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>